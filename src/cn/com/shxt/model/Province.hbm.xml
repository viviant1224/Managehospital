<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >
<hibernate-mapping package="cn.com.shxt.model">
	<class name="Province" table="PROVINCE" lazy="true">
		<id name="id">
			<column name="ID"></column>
			<generator class="sequence">
				<param name="sequence">PROVINCE_ID</param>
			</generator>
		</id>
	
		<property name="province">
			<column name="PROVINCE" length="10" unique="true"></column>
		</property>	
		
		<property name="provinceid">
			<column name="PROVINCEID" length="10"></column>
		</property>	
	</class>




</hibernate-mapping>
